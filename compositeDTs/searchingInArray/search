package main

func main() {
	names := [8]string{"name0", "name1", "name2", "name3", "name4", "name5", "name6", "name7"}

	searchKey := "name4"

	for i, name := range names {
		if name == searchKey {
			println("Name found. Index: ", i)
			break
		}
	}

	for j := 0; j < len(names); j++ {
		if names[j] == searchKey {
			println("Name found. Index: ", j)
			break
		}
	}
}